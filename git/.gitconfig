[advice]
  statusHints = false

[core]
  editor = "vim"
  excludesFile = "${HOME}/.gitignore"
  pager = "less"

[diff]
  algorithm = "histogram"
  colorMoved = "dimmed-zebra"

[fetch]
  prune = true
  pruneTags = true

[help]
  autocorrect = "prompt"

[pull]
  rebase = true

[push]
  autoSetupRemote = true
  default = "upstream"

[user]
  name = "Artur Dryomov"
